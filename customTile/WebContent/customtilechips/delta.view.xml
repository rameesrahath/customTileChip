<?xml version="1.0" encoding="UTF-8"?>
<core:View 
		xmlns="sap.suite.ui.commons" 
		xmlns:ui="sap.suite.ui.commons" 
		xmlns:core="sap.ui.core" 
		controllerName="customtilechips.delta">
		
     <GenericTile 
  	 	id="deltaMicro"
	 	press="onPress" 
	 	header="{/config/display_title_text}" 
	 	subheader="{/config/display_subtitle_text}" 
	 	frameType="TwoByOne">
        <tileContent>
        	<TileContent footer="{/config/display_info_text}">
            	<content>
                	<ui:DeltaMicroChart  value1 ="10.4" value2="20.5" title1="Start" title2="End" color="Good"/>
					
				</content>
			</TileContent>
		</tileContent>
	</GenericTile>
	
</core:View>