<?xml version="1.0" encoding="UTF-8"?>
<core:View 
		xmlns="sap.suite.ui.commons" 
		xmlns:ui="sap.suite.ui.commons" 
		xmlns:core="sap.ui.core" 
		controllerName="customtilechips.KpiTile">
		
     <GenericTile 
  	 	id="kpiTile"
	 	press="onPress" 
	 	header="{/config/display_title_text}" 
	 	subheader="{/config/display_subtitle_text}" 
	 	frameType="TwoByOne">
        <tileContent>
        	<TileContent footer="{/config/display_info_text}">
            	<content>
                	<ui:ComparisonChart scale="M">
						<ui:data>
							<!-- <ui:ComparisonData title="{/data/emp/month1}" value="{/data/emp/value1}" color="{/data/emp/color1}" />
							<ui:ComparisonData title="{/data/emp/month2}" value="{/data/emp/value2}" color="{/data/emp/color2}" />
							<ui:ComparisonData title="{/data/emp/month3}" value="{/data/emp/value3}" color="{/data/emp/color3}" /> -->
						<ui:ComparisonData title="data1" value="20" color="Good" />
							<ui:ComparisonData title="data2" value="120" color="Error" />
							<ui:ComparisonData title="data3" value="170" color="Good" />
						</ui:data>
					</ui:ComparisonChart>
				</content> 
			</TileContent>
		</tileContent>
	</GenericTile>
	
</core:View>