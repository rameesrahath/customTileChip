        <sapui5>
            <basePath>../zcustomtilechip</basePath>
            <viewName>customtilechips.delta.view.xml</viewName>
        </sapui5>
    </implementation>
    <appearance>
        <title>Delta Chart</title>
        <description>Dynamic Delta Micro Chip</description>
